<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="@color/white"/>
            <size
                android:height="8dp"
                android:width="8dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/dark_gray"/>
        </shape>

    </item>

    <item
        android:drawable="@drawable/baseline_wb_sunny_24"
        android:top="5dp"
        android:left="5dp"
        android:bottom="5dp"
        android:right="5dp"/>
</layer-list>